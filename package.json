{
  "name": "maildev",
  "description": "SMTP Server and Web Interface for reading and testing emails during development",
  "productName": "MailDev",
  "version": "0.14.0",
  "keywords": [
    "email",
    "e-mail",
    "mail",
    "mailcatcher"
  ],
  "author": "Dan Farrelly",
  "homepage": "http://djfarrelly.github.io/MailDev/",
  "maintainers": [
    {
      "name": "Dan Farrelly",
      "email": "daniel.j.farrelly@gmail.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "http://github.com/djfarrelly/maildev.git"
  },
  "scripts": {
    "start": "electron .",
    "test": "./node_modules/.bin/istanbul cover _mocha",
    "pack:osx": "rm -rf dist/osx && electron-packager . $npm_package_productName --out=dist/osx --platform=darwin --arch=x64 --icon=assets/icons/mail.icns --overwrite",
    "pack:win32": "electron-packager . $npm_package_productName --out=dist/win --platform=win32 --arch=ia32",
    "pack:win64": "electron-packager . $npm_package_productName --out=dist/win --platform=win32 --arch=x64 --version=0.14.0 app-version=1.0 --icon=--icon=assets/icons/mail.ico",
    "build": "npm run pack:osx && npm run pack:win32 && npm run pack:win64"
  },
  "main": "./main.js",
  "bin": {
    "maildev": "./bin/maildev"
  },
  "license": "MIT",
  "dependencies": {
    "async": "1.5.1",
    "commander": "2.9.0",
    "electron": "^1.4.0",
    "express": "4.13.4",
    "graceful-readlink": "^1.0.1",
    "mailparser": "0.5.3",
    "open": "0.0.5",
    "simplesmtp": "0.3.35",
    "smtp-connection": "2.3.1",
    "smtp-server": "1.4.0",
    "socket.io": "1.4.5",
    "wildstring": "1.0.8"
  },
  "devDependencies": {
    "grunt": "0.4.5",
    "grunt-concurrent": "2.2.1",
    "grunt-contrib-jshint": "1.0.0",
    "grunt-contrib-watch": "1.0.0",
    "grunt-nodemon": "0.4.1",
    "grunt-sass": "1.1.0",
    "http-proxy-middleware": "0.12.0",
    "istanbul": "0.4.4",
    "matchdep": "1.0.1",
    "mocha": "2.2.5",
    "nodemailer": "2.3.0",
    "request": "2.69.0"
  },
  "engines": {
    "node": ">=0.10.0"
  }
}
